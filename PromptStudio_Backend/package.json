{
  "name": "promptstudio_backend",
  "version": "1.0.0",
  "description": "PromptStudio Backend API - AI Prompt Generation Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "docker:build": "docker build -t promptstudio-backend .",
    "docker:run": "docker run -p 5000:5000 promptstudio-backend"
  },
  "keywords": [
    "ai",
    "prompt",
    "generator",
    "gemini",
    "api",
    "mern"
  ],
  "author": "PromptStudio Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/DHRUV-SHERE/PromptStudio"
  },
  "bugs": {
    "url": "https://github.com/DHRUV-SHERE/PromptStudio/issues"
  },
  "homepage": "https://promptstudio-av40.onrender.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.11",
    "validator": "^13.12.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}